<% provide(:title, 'Prdoucts Adding') %>
<h1>Product Add</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  
    <%= form_for(@product) do |f| %>
      <br/><%= f.label :name %>
   
     
      <br/><%= f.text_field :name %>
      
      <br/><%= f.label :brand_name%>
     
      <br/><%= f.text_field :brand_name%>
      
      <br/><%= f.label :url %>
     
      <br/><%= f.url_field :url %>
      
      <br/><%= f.label :price %>
     
      <br/><%= f.number_field :price %>
     
      <br/><%= f.label :quantity %>
      
      <br/><%= f.number_field :quantity %>
      
       
          <br/><%= f.submit "Add product", class: "btn btn-primary" %>
    <% end %>
  
  </div>
</div>