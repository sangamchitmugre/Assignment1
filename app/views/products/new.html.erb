


<% provide(:title, 'Prdoucts Adding') %>
<h1>Product Add</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  
    <%= form_for(@product) do |f| %>
      <%= f.label :name %>
   
     
      <%= f.text_field :name %>
      
      <%= f.label :brand_name%>
     
      <%= f.text_field :brand_name%>
      
      <%= f.label :url %>
     
      <%= f.url_field :url %>
      
      <%= f.label :price %>
     
      <%= f.number_field :price %>
     
      <%= f.label :quantity %>
      
      <%= f.number_field :quantity %>
      
       
          <%= f.submit "Add product", class: "btn btn-primary" %>
    <% end %>
  
  </div>
</div>